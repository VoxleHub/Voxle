Tab:CreateDropdown(
    {
        Name = "Weapons",
        Options = weaponTable,
        CurrentOption = "Select Weapon",
        Flag = "Dropdown1",
        Callback = function(selected)
            getgenv().weaponname = selected
        end
    }
)

Tab:CreateToggle(
    {
        Name = "Auto Level",
        CurrentValue = false,
        Flag = "Toggle1",
        Callback = function(bool)
            getgenv().autofarmquest = bool

            if bool then
                if wrk:FindFirstChild("Effects") then
                    wrk.Effects:Destroy()
                end
                for i, v in pairs(plr.Character:GetChildren()) do
                    if v.ClassName == "Tool" then
                        v.Parent = plr.Backpack
                    end
                end
            else
                if not wrk:FindFirstChild("Effects") then
                    local x = Instance.new("Folder", wrk)
                    x.Name = "Effects"
                end
            end

            wait(0.5)

            if not wrk:FindFirstChild("Effects") then
                spawn(
                    function()
                        while task.wait() do
                            if getgenv().autofarmquest then
                                if plr.leaderstats.Currents:GetAttribute("Quest") == "" then
                                    getQuest(getLevel())
                                    if getgenv().equiptool then
                                        statFarm(getgenv().weapon, getgenv().defence)
                                    end
                                else
                                    if
                                        getgenv().weaponname and getgenv().weaponname ~= "Select A Weapon Pls!" and
                                            getgenv().weaponname ~= nil
                                     then
                                        getId()
                                        if getgenv().equiptool then
                                            statFarm(getgenv().weapon, getgenv().defence)
                                        end
                                        for i, x in pairs(wrk.__GAME.__Mobs:GetChildren()) do
                                            if string.match(x.Name, getgenv().id) then
                                                for i, v in pairs(x:GetChildren()) do
                                                    if plr.leaderstats.Currents:GetAttribute("Quest") == 1 then
                                                        if
                                                            string.find(v.Name, "Bandit") and
                                                                not string.find(v.Name, "StrongBandit") and
                                                                not string.find(v.Name, "BanditLeader")
                                                         then
                                                            if
                                                                v:FindFirstChild("NpcModel") and
                                                                    v:FindFirstChild("NpcHealth")
                                                             then
                                                                if
                                                                    v.NpcModel:FindFirstChild("HumanoidRootPart") and
                                                                        string.split(
                                                                            v.NpcHealth.ViewerFrame.Frame.HealthText.Text,
                                                                            "/"
                                                                        )[1] ~= "0"
                                                                 then
                                                                    if getgenv().autofarmquest then
                                                                        repeat
                                                                            task.wait()
                                                                            if getgenv().equiptool then
                                                                                plr.Character:WaitForChild(
                                                                                        "HumanoidRootPart"
                                                                                    ).CFrame =
                                                                                    v.NpcModel.HumanoidRootPart.CFrame
                                                                                statFarm(
                                                                                    getgenv().weapon,
                                                                                    getgenv().defence
                                                                                )
                                                                            end
                                                                        until not getgenv().autofarmquest or
                                                                            string.split(
                                                                                v.NpcHealth.ViewerFrame.Frame.HealthText.Text,
                                                                                "/"
                                                                            )[1] == "0" or
                                                                            plr.leaderstats.Currents:GetAttribute(
                                                                                "Quest"
                                                                            ) == ""
                                                                    end
                                                                end
                                                            end
                                                        end
                                                    elseif plr.leaderstats.Currents:GetAttribute("Quest") == 2 then
                                                        if string.find(v.Name, "StrongBandit") then
                                                            if
                                                                v:FindFirstChild("NpcModel") and
                                                                    v:FindFirstChild("NpcHealth")
                                                             then
                                                                if
