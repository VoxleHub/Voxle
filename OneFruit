    {
        Name = "Auto Punch",
        CurrentValue = false,
        Flag = "Toggle2",
        Callback = function(Value)
            a = Value
            while a do
                task.wait()
                vu:Button1Down(Vector2.new(1000, 1000), workspace.CurrentCamera.CFrame)
                wait(0.1)
                vu:Button1Up(Vector2.new(1000, 1000), workspace.CurrentCamera.CFrame)
            end
        end
    }
)

